//Write a C Program to compute the multiplication table of 1 - 5 using do-while.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

int n,i=1,m;

do{
    printf("\n1.Table of 1\n2.Table of 2\n3.Table of 3\n4.Table of 4\n5.Table of 5\n6.Exit\n");
    scanf("%d",&n);
    if(n>6)
    {
        printf("Enter a valid option!");
    }
    else
    {
        if(n==6)
        {
            exit(0);
        }
        do{
            m=n*i;
            printf("%d ",m);
            i++;
        }while(i<=10);
    }
   
 }while(1);
return 0;
}
