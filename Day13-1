//Write a program in C to Find the Frequency of Characters.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char s[100],ch;
    int i,count=0;
    fgets(s,sizeof(s),stdin);
    scanf("%c",&ch);
    for(i=0;s[i]!='\0';i++)
    {
        if(ch==s[i]){
        count++;
        }
    }
    printf("%d",count);
    return 0;
}
