//Write a C program to print the inverted half pyramid number pattern

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int r,c,i,j,k;
    scanf("%d",&r);
    c=r;
    for(i=1;i<=r;i++)
    {
        for(k=1;k<i;k++)
        {
            printf(" ");
        }
        for(j=1;j<=c;j++)
        {
                printf("%d",c);
        }
        c--;
        printf("\n");
        
    }
    return 0;
}
