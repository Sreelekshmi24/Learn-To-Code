//Write a C program to print the Fibonocci series starting from 0 and the upperlimit is determined by the user

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,i,a=0,b=1,c;
    scanf("%d",&n);
    printf("%d %d ",a,b);
    for(i=2;i<n;++i)
    {
        c=a+b;
        printf("%d ",c);
        a=b;
        b=c;
    }
    return 0;
}
