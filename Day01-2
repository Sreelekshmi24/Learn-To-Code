//Write a C Program to perform Read and Print operations using the four basic data types in C. (int, float, char & double)

#include <stdio.h>
int main()
{
  char ch;
  scanf("%c", &ch);
  printf("%c\n", ch);
  int i;
  scanf("%d", &i);
  printf("%d\n", i);
  float f;
  scanf("%f", &f);
  printf("%f\n", f);
  double d;
  scanf("%lf", &d);
  printf("%lf\n", d);
  
}
