//Write a C program to find the frequency of odd and even numbers in the 1D array.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

        int arr[10];
        int i,n,even = 0,odd = 0;
        scanf("%d",&n);
        for (i = 0; i < n; ++i) 
        {
            scanf("%d", &arr[i]);
            if ((arr[i] % 2) == 0)
            {
                 ++even;
            }
            else
                ++odd;
        }
 
        printf("%d\n",even);
        printf("%d",odd);
    
    return 0;
}
